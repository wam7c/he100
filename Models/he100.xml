  <!-- propeller -->
  <animation>
    <name>PropellerComplete</name>
    <object-name>propeller</object-name>
    <object-name>nosecone</object-name>
  </animation>
  
  <!-- Séléction de l'objet visible en fonction des rpm 
removed propblur and propdisk 
-->

  
  <animation>
    <type>select</type>
    <object-name>propeller</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>900</value>
      </less-than>
    </condition>
  </animation>
  
  <animation>
    <type>spin</type>
    <object-name>PropellerComplete</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value> 5 </value>
      </greater-than>
    </condition>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -3.8 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  1.870 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <!-- Animations des ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>leftaileron</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
      <entry><ind> -0.20 </ind><dep> -2.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.2</ind><dep>  2.0 </dep></entry>
    </interpolation>
    <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>rightaileron</object-name>
    <property>surface-positions/right-aileron-pos-norm</property>
    <interpolation>
      <entry><ind> -0.2 </ind><dep> -2.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.2 </ind><dep>  2.0 </dep></entry>
    </interpolation>
    <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
    </axis>
  </animation>
  
  <!-- Volets are flaps -->
  <animation>
    <type>rotate</type>
    <object-name>rightflap</object-name>
    <property>surface-positions/flaps-pos-norm</property>
    <factor> 20 </factor>
    <axis>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>leftflap</object-name>
    <property>surface-positions/flaps-pos-norm</property>
    <factor> 20 </factor>
    <axis>
  <axis>
   <x>10</x>
   <y>0</y>
   <z>0</z>
  </axis>
    </axis>
  </animation>
  
  <!-- Gouverne de elevator -->
  <animation>
    <type>rotate</type>
    <object-name>elevator</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <factor> 3 </factor>
    <center>
      <x-m> 3.0 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.434 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1</y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <!-- Gouverne de rudder -->
  <animation>
    <type>rotate</type>
    <object-name>rudder</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor> -13 </factor>
    <center>
      <x-m> 4.0 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 7.639 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>
  
  <!-- Compression removed here-->

  
