      </equals>
      </condition>
      <object-name>wheelstrutleft</object-name>
      <object-name>wheelstrutpropleft</object-name>
     </animation>
  
    <animation>
      <type>select</type>
      <object-name>rightgear</object-name>
      <condition>
       <greater-than>
        <property>gear/gear[0]/position-norm</property>
        <value>0</value>
